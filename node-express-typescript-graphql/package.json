{
  "name": "music-streamer",
  "version": "1.0.0",
  "description": "Server that gets music from yt or spotify via youtube-dl and then streams it via sockets",
  "main": "index.ts",
  "scripts": {
    "start": "npm run build && node -r ts-node/register/transpile-only -r tsconfig-paths/register build/index.js",
    "dev": "nodemon",
    "build": "rimraf ./build && tsc",
    "lint": "eslint . --ext .ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "martimalek",
  "license": "ISC",
  "devDependencies": {
    "@graphql-eslint/eslint-plugin": "^0.9.1",
    "@types/express": "^4.17.11",
    "@types/node": "^15.0.3",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "eslint": "^7.26.0",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "typescript-transform-paths": "^2.2.3"
  },
  "dependencies": {
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.5.0",
    "graphql-import-node": "^0.0.4",
    "graphql-tools": "^7.0.5",
    "tsconfig-paths": "^3.9.0",
    "youtube-dl-exec": "^1.2.3"
  }
}
